    "@babel/preset-env": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "lodash.merge": "^4.6.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
